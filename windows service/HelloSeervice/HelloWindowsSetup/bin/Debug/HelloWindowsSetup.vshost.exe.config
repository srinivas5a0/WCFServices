<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="MexBehavior">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="MexBehavior" name="HelloSeervice.HelloService">
        <endpoint address="HelloService" binding="netTcpBinding" bindingConfiguration=""
            contract="HelloSeervice.IHelloService" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:80" />
            <add baseAddress="net.tcp://localhost:90" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>